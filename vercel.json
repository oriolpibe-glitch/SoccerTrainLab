{
  "version": 2,
  "builds": [
    {
      "src": "public/**/*.html",
      "use": "@vercel/static"
    },
    {
      "src": "public/**/*.js",
      "use": "@vercel/static"
    },
    {
      "src": "public/**/*.css",
      "use": "@vercel/static"
    },
    {
      "src": "public/assets/**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/",
      "dest": "/public/index.html"
    },
    {
      "src": "/login",
      "dest": "/public/login.html"
    },
    {
      "src": "/dashboard-entrenador",
      "dest": "/public/dashboard-entrenador.html"
    },
    {
      "src": "/dashboard-familiar",
      "dest": "/public/dashboard-familiar.html"
    },
    {
      "src": "/admin",
      "dest": "/public/admin.html"
    },
    {
      "src": "/biblioteca",
      "dest": "/public/biblioteca.html"
    },
    {
      "src": "/biblioteca-personal",
      "dest": "/public/biblioteca_personal.html"
    },
    {
      "src": "/crear-ejercicio",
      "dest": "/public/crearejercicio.html"
    },
    {
      "src": "/partidos",
      "dest": "/public/partits.html"
    },
    {
      "src": "/planificacion",
      "dest": "/public/planificacio.html"
    },
    {
      "src": "/(.*)",
      "dest": "/public/$1"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Content-Security-Policy",
          "value": "default-src 'self' https://www.gstatic.com https://fonts.googleapis.com https://cdnjs.cloudflare.com https://fonts.gstatic.com 'unsafe-inline' 'unsafe-eval' blob: data:;"
        },
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "Referrer-Policy",
          "value": "strict-origin-when-cross-origin"
        }
      ]
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
}